{
  "name": "pay-advance",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^3.0.2",
    "aws-amplify": "1.1.10",
    "chart.js": "^2.8.0",
    "clsx": "^1.0.4",
    "connected-react-router": "^4.5.0",
    "foundation-sites": "^6.5.3",
    "lodash": "^4.17.14",
    "material-table": "^1.40.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^5.1.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.4",
    "redux-saga": "^0.16.0",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "npm-run-all -p dev",
    "build": "react-scripts build",
    "deploy": "aws s3 sync build/ s3://payadvance-test.app-drive.co.uk",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}'",
    "build-css": "node-sass-chokidar src/css -o src/css --recursive --include-path ./node_modules",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o src/css --watch --recursive --include-path ./node_modules"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.2",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.10.1",
    "stylelint-scss": "^3.9.1"
  }
}
